{
  "name": "t-travel",
  "version": "1.0.0",
  "description": "Decentralized travel booking platform built on Stacks blockchain",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "dependencies": {
    "@stacks/connect": "^7.8.2",
    "@stacks/transactions": "^6.13.1",
    "@hirosystems/chainhooks-client": "^1.6.0",
    "@walletconnect/web3wallet": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "@stacks/auth": "^6.11.3",
    "@stacks/network": "^6.13.0",
    "@stacks/stacks-blockchain-api-types": "^7.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5"
  },
  "keywords": [
    "stacks",
    "blockchain",
    "travel",
    "booking",
    "clarity",
    "web3",
    "decentralized"
  ],
  "author": "T-travel Team",
  "license": "MIT"
}
